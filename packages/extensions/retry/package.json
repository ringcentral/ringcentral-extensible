{
  "name": "@rc-ex/retry",
  "version": "1.3.8",
  "description": "Retry extension for ringcentral-extensible project",
  "author": "Tyler Liu <tyler.liu@ringcentral.com>",
  "homepage": "https://github.com/ringcentral/ringcentral-extensible/tree/master/packages/extensions/retry",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/ringcentral-extensible.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-extensible/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "wait-for-async": "^0.7.10"
  },
  "scripts": {
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json"
  }
}
