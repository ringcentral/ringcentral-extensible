{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "ESNext",
    "target": "ESNext",
    "declaration": true,
    "outDir": "./dist/esm",
    "skipLibCheck": true,
    "paths": {
      "@rc-ex/core": [
        "../../core/dist/esm/index"
      ],
      "@rc-ex/core/*": [
        "../../core/dist/esm/*"
      ],
      "@ringcentral/sdk": [
        "../../../node_modules/@ringcentral/sdk/es6/index.js"
      ]
    }
  },
  "include": ["src/**/*.ts"]
}
