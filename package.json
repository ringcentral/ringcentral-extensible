{
  "name": "ringcentral-unified",
  "version": "0.6.0",
  "description": "RingCentral Unified SDK for TypeScript",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "git@github.com:ringcentral/ringcentral-unified-ts.git",
  "author": "Tyler Liu <tyler.liu@ringcentral.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest -w 1 --detectOpenHandles",
    "definitions": "rm -rf ./src/definitions/* && node -r dotenv-override-true/config ./code-generator/definitions.js",
    "paths": "rm -rf ./src/paths/* && node -r dotenv-override-true/config ./code-generator/paths.js",
    "generate": "yarn definitions && yarn paths && gts fix",
    "prepublishOnly": "rm -rf build && tsc && rm build/package.json && mv build/src/* build/ && rmdir build/src && gts fix && yarn test"
  },
  "dependencies": {
    "@types/qs": "^6.9.3",
    "@types/ws": "^7.2.4",
    "axios": "^0.19.2",
    "form-data": "^3.0.0",
    "http-status-codes": "^1.4.0",
    "hyperid": "^2.0.5",
    "isomorphic-ws": "^4.0.1",
    "qs": "^6.9.4",
    "wait-for-async": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.4",
    "change-case": "^4.1.1",
    "dotenv-override-true": "^6.2.0",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lower-case-first": "^2.0.1",
    "ramda": "^0.27.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "yarn-upgrade-all": "^0.5.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "gts fix && yarn test"
    }
  }
}
